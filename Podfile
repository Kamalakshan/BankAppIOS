  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  # Pods for BankAppIOS
  use_frameworks!
  pod 'Alamofire', '~> 4.0'
  pod 'SwiftyJSON', :git => 'https://github.com/BaiduHiDeviOS/SwiftyJSON.git', :branch => 'swift3'

